class Solution {
public:
    string gcdOfStrings(string str1, string str2) {
        set<char> st1;
        set<char> st2;
        string ans = "";
        for(auto ch : str1){
            st1.insert(ch);
        }
        for(auto ch : str2){
            st2.insert(ch);
        }
        for(int i = 0; i < st2.size(); i++){
            if(st1 != st2){
                return ans;
            }
            ans = st2;
        }
        return ans;
    }
};
