class Solution {
public:

    void findSet(int index, vector<vector<int>>& ans, vector<int>& nums, vector<int>& ds){
        ans.push_back(ds);
        for(int i = index; i < nums.size(); i++){
            if(i != index && nums[i] == nums[i-1]){
                continue;
            }
            ds.push_back(nums[i]);
            findSet(i+1, ans, nums, ds);
            ds.pop_back();
        }
    }

    vector<vector<int>> subsetsWithDup(vector<int>& nums) {
        vector<vector<int>> ans;
        vector<int> ds;
        sort(nums.begin(), nums.end());
        findSet(0,ans,nums,ds);
        return ans;
    }
};
